{
  "name": "banner-slot",
  "version": "0.1.1",
  "description": "Banner slot UI component for React Native / Expo / React Native Web.",
  "license": "Apache-2.0",
  "private": false,
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "react-native": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup --config src/tsup.config.ts",
    "prepack": "npm run build",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-native": ">=0.72",
    "react-native-web": ">=0.19"
  },
  "peerDependenciesMeta": {
    "react-native-web": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "react": "^19.2.3",
    "react-native": "^0.83.1",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
